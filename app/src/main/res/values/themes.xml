<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.WholeTrack" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/whiteTint</item>
        <item name="colorPrimaryVariant">@color/cardview_dark_background</item>
        <item name="colorOnPrimary">@color/darkPurple</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/darkPurple</item>
        <item name="colorSecondaryVariant">@color/bluePurple</item>
        <item name="colorOnSecondary">@color/lightPurple</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>